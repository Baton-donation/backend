{
  "rootDir": "..",
  "roots": ["<rootDir>/src/", "<rootDir>/test/"],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1"
  },
  "preset": "ts-jest",
  "coveragePathIgnorePatterns": ["/node_modules/", "/test/"],
  "coverageDirectory": "coverage"
}
